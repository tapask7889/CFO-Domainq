        <errorMessage>&quot;Do Not Send Reason&quot; Field is a Required if &quot;Do Not Send &quot; value is selected in picklist for &quot;Email Status&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_Not_call_reason</fullName>
